public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    //code here
    Integer passedCount = 0;
    if (passedExam) {
        passedCount++;
    }
    if (passedAssignments) {
        passedCount++;
    }
    if (passedProject) {
        passedCount++;
    }
    return passedCount >= 2;
}