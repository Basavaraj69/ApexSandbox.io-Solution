public Id classFromCourse(String courseName) {
    // code here

apxio__Course__c  course = [SELECT Id, Name FROM apxio__Course__c  WHERE Name = : courseName];

if(course == null){
    return null;
}
   


 apxio__Class__c  newRecord = new apxio__Class__c();
    newRecord.Name = courseName;
    newRecord.apxio__Course__c = course.Id;

     insert newRecord;

     return newRecord.Id;
}

    