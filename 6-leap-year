public Boolean isLeapYear(Integer year) {
    //code here
    Integer Year =  Math.mod(year, 4);
    Integer Year1 =  Math.mod(year, 100);
    Integer Year2 =  Math.mod(year, 400);

    if(Year != 0){
       return false;
    }else if( Year1 != 0){
        return false;
    }else if( Year2 != 0){
        return false;
    }else{
        return true;
    }
}
